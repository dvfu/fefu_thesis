            \@FEFUpostunderline{Виды работ и требования по их выполнению: }{\textwidth}\\
            \foreach \n in {1,...,#2}{\underline{\hspace{\textwidth}}\\}
        }{Виды работ и требования по их выполнению: {\expandafter\@FEFUunderline\@FEFUpracticetask}}
