% \begin{macro}{\@FEFUcredentials}
% Insert a field for person's credentials.
%    \begin{macrocode}
\newcommand{\@FEFUcredentials}[4][\textwidth]{%
    \setlength{\@FEFUlength}{#1-\widthof{#2 }}%
    \settowidth{\@FEFUcredentialslength}{#3, #4 \hspace{\@FEFUsignwidth}}%
    \ifthenelse{\lengthtest{\@FEFUlength<\@FEFUcredentialslength}}{%
        #2\\
        \@FEFUlabeled{\uline{\mbox{\hbox to #1{#3, #4\hfill}}}}{Ф.И.О., должность, подпись}%
    }{%
        #2 \@FEFUlabeled{\uline{\mbox{\hbox to \@FEFUlength{#3, #4\hfill}}}}{Ф.И.О., должность, подпись}%
    }%
}
