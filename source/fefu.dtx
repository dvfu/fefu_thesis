% \begin{macro}{\@FEFUshortyear}
% Prints short year.
%    \begin{macrocode}
\newcommand*\@FEFUshortyear[1]{\expandafter\@gobbletwo\number\numexpr#1\relax}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUshortname}
% Prints short name.
%    \begin{macrocode}
\def\@FEFUshortnamehelper#1 #2 #3\relax{%
    \StrLeft{#2}{1}[\@FEFU@name]\StrLeft{#3}{1}[\@FEFU@middlename]%
    #1 \@FEFU@name. \@FEFU@middlename.%
}
\def\@FEFUshortname#1{\@FEFUifempty{#1}{}{\expandafter\@FEFUshortnamehelper#1\relax}}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUshortnamereversed}
% Prints short name, surname last
%    \begin{macrocode}
\def\@FEFUshortnamereversedhelper#1 #2 #3\relax{%
    \StrLeft{#2}{1}[\@FEFU@name]\StrLeft{#3}{1}[\@FEFU@middlename]%
    \@FEFU@name. \@FEFU@middlename. #1%
}
\def\@FEFUshortnamereversed#1{\@FEFUifempty{#1}{}{\expandafter\@FEFUshortnamereversedhelper#1\relax}}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUgetyear}
% Extracts year from dd.mm.yy or uses default one
%    \begin{macrocode}
\def\@FEFUdropdot#1.\relax{#1}
\newcommand{\@FEFUgetyear}[2][\the\year]{%
    \def\@FEFUgetyearhelper##1.##2.##3\relax{\@FEFUifempty{##3}{#1}{20\@FEFUdropdot##3\relax}}%
    \expandafter\@FEFUgetyearhelper#2.\relax%
}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUgetmonth}
% Gets month from dd.mm or dd.mm.yy
%    \begin{macrocode}
\def\@FEFUgetmonthhelper#1.#2.#3\relax{#2}
\def\@FEFUgetmonth#1{\expandafter\@FEFUgetmonthhelper#1.\relax}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUgetday}
% Gets day from dd.mm or dd.mm.yy
%    \begin{macrocode}
\def\@FEFUgetdayhelper#1.#2\relax{#1}
\def\@FEFUgetday#1{\expandafter\@FEFUgetdayhelper#1\relax}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUifempty}
% Selects \marg{arg2} or \marg{arg3} depending on whether \marg{arg1} is empty or not.
%    \begin{macrocode}
\newcommand{\@FEFUifempty}[3]{%
    \ifthenelse{\equal{#1}{}}{#2}{#3}%
}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUcoalesce}
% Returns first \marg{arg1} if it's not empty otherwise returns \marg{arg2}.
%    \begin{macrocode}
\newcommand{\@FEFUcoalesce}[2]{%
    \@FEFUifempty{#1}{\@FEFUifempty{#2}{}{#2}}{#1}%
}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUdate}
% Inserts date field.
%    \begin{macrocode}
\newcommand{\@FEFUdate}[2][]{%
    \setlength{\@FEFUlength}{#2-\widthof{\guillemotleft\underline{\hspace{1cm}}\guillemotright\ \ \the\year\ г.}}%
    \@FEFUifempty{#1}{%
        \guillemotleft\underline{\hspace{1cm}}\guillemotright \underline{\hspace{\@FEFUlength}} \the\year\ г.%
    }{%
        \guillemotleft\underline{\hbox to 1cm{\hfill\@FEFUgetday{#1}\hfill}}\guillemotright \underline{\hbox to \@FEFUlength{\hfill\csname @FEFUmonthname\@FEFUgetmonth{#1}\endcsname\hfill}} \@FEFUgetyear{#1}\ г.
    }%
}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUprettydate}
% Prints date with month name
%    \begin{macrocode}
\newcommand{\@FEFUprettydate}[1]{%
    \@FEFUifempty{#1}{}{\@FEFUgetday{#1} \csname @FEFUmonthname\@FEFUgetmonth{#1}\endcsname}
}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUlabeled}
% Labels \marg{arg1} with a label \marg{arg2} if it's not empty
%    \begin{macrocode}
\newcommand{\@FEFUlabeled}[2]{%
    \@FEFUifempty{#2}{%
        #1%
    }{%
        \stackunder{#1}{{\scriptsize(#2)}}%
    }%
}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUfield}
% Inserts a labeled field.
%    \begin{macrocode}
\newcommand{\@FEFUfield}[3][]{%
    \settowidth{\@FEFUfieldlength}{#2}%
    \@FEFUlabeled{\uline{\mbox{\ifthenelse{\lengthtest{\@FEFUfieldlength<#3}}{\hbox to #3{\hfill#2\hfill}}{#2}}}}{#1}%
}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUpostfield}
% Inserts \marg{arg1} and fills avaliable space with a field.
%    \begin{macrocode}
\newcommand{\@FEFUpostfield}[4][]{%
    \setlength{\@FEFUpostfieldlength}{#4-\widthof{#2}}%
    #2\@FEFUfield[#1]{#3}{\@FEFUpostfieldlength}%
}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUmakedate}
% Adds year to a date if date is set.
%    \begin{macrocode}
\newcommand{\@FEFUmakedate}[3][true]{%
    \@FEFUifempty{#2}{}{#2.\ifthenelse{\equal{#1}{true}}{\@FEFUshortyear{#3}}{#3}}%
}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\@FEFUdaterange}
% Prints date range.
%    \begin{macrocode}
\newcommand{\@FEFUdaterange}[4]{%
    \ifthenelse{\equal{#1}{true}}{%
        с \@FEFUfield{\@FEFUmakedate[#2]{\@FEFUstartdate}{\@FEFUstartyear}}{#4}
        по \@FEFUfield{\@FEFUmakedate[#2]{\@FEFUenddate}{\@FEFUendyear}}{#4}%
    }{%
        \ifthenelse{\equal{#3}{true}}{%
            с \@FEFUfield{\@FEFUprettydate{\@FEFUstartdate}}{#4}
            по \@FEFUfield{\@FEFUprettydate{\@FEFUenddate}}{#4}%   
        }{%
            с \@FEFUfield{\@FEFUstartdate}{#4}
            по \@FEFUfield{\@FEFUenddate}{#4}%
        }%
    }%
}
%    \end{macrocode}
% \end{macro}
